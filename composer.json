{
    "name": "ksarien/l5-db-commands",
    "type": "library",
    "description": "A set of commands to create/drop/dump/restore databases on Laravel 5",
    "keywords": ["laravel", "laravel 5", "database", "commmands"],
    "homepage": "https://github.com/ksarien/l5-db-commands",
    "license": "MIT",
    "authors": [
        {
            "name": "Maycon Viana Bordin",
            "email": "mayconbordin@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/console": "~5.3.0|~5.4.0|~5.5.0",
        "illuminate/support": "~5.3.0|~5.4.0|~5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5.7|^6.2"
    },
    "autoload": {
        "psr-4": {
            "Mayconbordin\\DbCommands\\": "src/Mayconbordin/DbCommands/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Mayconbordin\\DbCommands\\DbCommandsServiceProvider"
            ]
        }
    }
}
